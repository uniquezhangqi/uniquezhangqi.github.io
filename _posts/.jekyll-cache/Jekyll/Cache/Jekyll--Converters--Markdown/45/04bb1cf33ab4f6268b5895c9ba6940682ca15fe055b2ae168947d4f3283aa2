  <span class="cm">/**
	* 
	* @Title: sendPostJson
	* @Description: é€šç”¨POST,JSONè¯·æ±‚
	* @param url
	* @param json
	* @return
	*/</span>
	<span class="kd">public</span> <span class="kd">static</span> <span class="nc">JSONObject</span> <span class="nf">sendPostJson</span><span class="o">(</span><span class="nc">String</span> <span class="n">url</span><span class="o">,</span> <span class="nc">String</span> <span class="n">json</span><span class="o">)</span> <span class="kd">throws</span> <span class="nc">Exception</span> 
	<span class="o">{</span>
	
		<span class="no">LOGGER</span><span class="o">.</span><span class="na">info</span><span class="o">(</span><span class="s">"è¯·æ±‚url:"</span><span class="o">+</span><span class="n">url</span><span class="o">);</span>
